<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace：命名空间，用于隔离sql-->
<mapper namespace="cn.stonemind.generate.mapper.UserMapper">
    <resultMap id="user" type="cn.stonemind.entity.User">
        <result></result>

    </resultMap>

    <select id="getUserList" resultType="cn.stonemind.entity.User">
        select * from user;
    </select>

</mapper>